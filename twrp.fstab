/system      ext4    /dev/block/platform/7824900.sdhci/by-name/system
/data        ext4    /dev/block/platform/7824900.sdhci/by-name/userdata      length=-16384
/cache       ext4    /dev/block/platform/7824900.sdhci/by-name/cache
/boot        emmc    /dev/block/platform/7824900.sdhci/by-name/boot
/recovery    emmc    /dev/block/platform/7824900.sdhci/by-name/recovery      flags=backup=1
/modem       emmc    /dev/block/platform/7824900.sdhci/by-name/modem
/external_sd		vfat	/dev/block/mmcblk1p1			flags=display="External SDcard";storage;wipeingui;removable
/usb-otg      auto    /dev/block/sda1        /dev/block/sda             flags=display="USB OTG";storage;wipeingui;removable
